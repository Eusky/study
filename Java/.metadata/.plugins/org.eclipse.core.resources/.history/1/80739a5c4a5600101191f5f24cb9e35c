package chap07_access_modifier;

public class Brand {
  // 필드
  private String name; //--------- 브랜드 이름
  private String nation; //------- 브랜드 국적
  
  // 생성자
  public Brand() {
    this("Benz", "Germany");
  }
  
  public Brand(String name, String nation) {
    this.name = name;
    this.nation = nation;
  }

  // 메소드
  public void printInfo() {
    System.out.println(name + " " + nation);
  }
}
